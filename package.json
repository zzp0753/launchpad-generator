{
  "name": "launchpad-generator",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@9.11.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "prepare": "husky",
    "postinstall": "pnpm --filter @packages/* build"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "eslint": "^9.13.0",
    "husky": "^9.1.6",
    "prettier": "^3.3.3",
    "turbo": "^2.1.1",
    "typescript": "^5.6.2"
  }
}
